{
  "name": "diagnostics-sv",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "npm-run-all check:sync check:type",
    "check:sync": "svelte-kit sync",
    "check:type": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm-run-all check:sync check:watch:type",
    "check:watch:type": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "npm-run-all lint:prettier lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.36.2",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/svelte-query": "^5.85.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/node": "^22.17.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.12.6",
    "flowbite-svelte-icons": "^2.3.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "mock-match-media": "^0.4.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.38.3",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@sveltejs/kit": {
      "cookie": "^0.7.0"
    }
  }
}
