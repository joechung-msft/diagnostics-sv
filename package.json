{
  "name": "diagnostics-sv",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "npm-run-all check:sync check:type",
    "check:sync": "svelte-kit sync",
    "check:type": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npm-run-all check:sync check:watch:type",
    "check:watch:type": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "npm-run-all lint:prettier lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^1.11.3",
    "flowbite-svelte-icons": "^2.2.1",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "vite-plugin-devtools-json": "^0.2.0"
  },
  "overrides": {
    "@sveltejs/kit": {
      "cookie": "^0.7.0"
    }
  },
  "dependencies": {
    "@tanstack/svelte-query": "^5.83.1"
  }
}
